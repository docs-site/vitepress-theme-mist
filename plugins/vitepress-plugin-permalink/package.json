{
  "name": "@docs-site/vitepress-plugin-permalink",
  "type": "module",
  "version": "1.0.1",
  "description": "扫描 Markdown 文档，生成永久链接",
  "author": "",
  "keywords": [
    "vitepress",
    "vite",
    "plugin",
    "permalink"
  ],
  "license": "MIT",
  "homepage": "https://github.com/docs-site/vitepress-theme-mist/tree/master/plugins/vitepress-plugin-permalink",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/docs-site/vitepress-theme-mist.git"
  },
  "bugs": {
    "url": "https://github.com/docs-site/vitepress-theme-mist/issues"
  },
  "files": [
    "dist",
    "README.md",
    "package.json"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./usePermalink": {
      "types": "./dist/usePermalink.d.ts",
      "import": "./dist/usePermalink.mjs",
      "require": "./dist/usePermalink.cjs"
    },
    "./rewrites": {
      "types": "./dist/rewrites.d.ts",
      "import": "./dist/rewrites.mjs",
      "require": "./dist/rewrites.cjs"
    },
    "./dist/components/*.vue": "./dist/components/*.vue"
  },
  "typesVersions": {
    "*": {
      "*": [
        "*",
        "dist/*",
        "dist/*.d.ts",
        "dist/*/index.d.ts"
      ]
    }
  },
  "scripts": {
    "stub": "unbuild --stub",
    "build": "unbuild"
  },
  "dependencies": {
    "gray-matter": "catalog:",
    "picocolors": "catalog:"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "fs-extra": "^11.2.0",
    "vite": "catalog:",
    "vitepress": "catalog:"
  },
  "peerDependencies": {
    "vitepress": "catalog:"
  },
  "publishConfig": {
    "access": "public"
  }
}
