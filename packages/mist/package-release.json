{
  "name": "vitepress-theme-mist",
  "version": "1.0.0",
  "description": "vitepress-theme-mist",
  "type": "module",
  "author": "sumu",
  "keywords": [
    "vitepress"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "module": "es/index.mjs",
  "types": "es/index.d.ts",
  "style": "index.css",
  "exports": {
    ".": {
      "types": "./es/index.d.ts",
      "import": "./es/index.mjs",
      "require": "./lib/index.js"
    },
    "./es": {
      "types": "./es/index.d.ts",
      "import": "./es/index.mjs"
    },
    "./es/*.mjs": {
      "types": "./es/*.d.ts",
      "import": "./es/*.mjs"
    },
    "./es/*": {
      "types": [
        "./es/*.d.ts",
        "./es/*/index.d.ts"
      ],
      "import": "./es/*.mjs"
    },
    "./lib": {
      "types": "./lib/index.d.ts",
      "require": "./lib/index.js"
    },
    "./lib/*.js": {
      "types": "./lib/*.d.ts",
      "require": "./lib/*.js"
    },
    "./lib/*": {
      "types": [
        "./lib/*.d.ts",
        "./lib/*/index.d.ts"
      ],
      "require": "./lib/*.js"
    },
    "./config": {
      "types": "./es/config/index.d.ts",
      "import": "./es/config/index.mjs"
    },
    "./*": "./*"
  },
  "homepage": "https://github.com/docs-site/vitepress-theme-mist/tree/master",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/docs-site/vitepress-theme-mist.git"
  },
  "bugs": {
    "url": "https://github.com/docs-site/vitepress-theme-mist/issues"
  },
  "dependencies": {
    "@iconify/vue": "^5.0.0",
    "js-yaml": "^4.1.0",
    "markdown-it-container": "^4.0.0",
    "@docs-site/vitepress-plugin-file-content-loader": "^1.0.3",
    "@docs-site/vitepress-plugin-catalogue": "^1.0.5",
    "@docs-site/vitepress-plugin-doc-analysis": "^1.0.2",
    "@docs-site/vitepress-plugin-demo": "^1.0.1",
    "@docs-site/vitepress-auto-nav-sidebar": "^1.0.4",
    "@docs-site/vitepress-plugin-permalink": "^1.0.2"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/markdown-it": "^14.1.2",
    "@types/markdown-it-container": "^2.0.10",
    "vitepress": "^1.6.4"
  },
  "peerDependencies": {
    "vitepress": "^1.6.4"
  },
  "browserslist": [
    "> 1%",
    "not ie 11",
    "not op_mini all"
  ]
}