{
  "version": "1.0.0",
  "description": "vitepress-theme-mist root package",
  "type": "module",
  "private": true,
  "scripts": {
    "docs:dev": "pnpm run -C docs docs:dev",
    "docs:build": "pnpm run -C docs docs:build",
    "docs:preview": "pnpm run -C docs docs:preview",
    "kill:node": "taskkill /f /im node.exe",
    "build": "pnpm run clean && pnpm run to:build build",
    "publish": "bash build/scripts/publish.sh",
    "clean": "rimraf dist",
    "clean:all": "node build/scripts/clean.js",
    "to:build": "pnpm run -C build",
    "stub": "pnpm run to:build stub && pnpm run packages:stub && pnpm run plugins:stub",
    "packages:stub": "pnpm -r --filter=./packages/* run stub",
    "plugins:stub": "pnpm -r --filter=./plugins/* run stub",
    "plugins:build": "pnpm -r --filter=./plugins/* run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "packageManager": "pnpm@10.14.0",
  "dependencies": {
    "@mist/components": "workspace:*",
    "@mist/composables": "workspace:*",
    "@mist/config": "workspace:*",
    "@mist/helper": "workspace:*",
    "@mist/locale": "workspace:*",
    "@mist/markdown": "workspace:*",
    "@mist/static": "workspace:*",
    "@mist/theme-chalk": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "@vue/runtime-core": "catalog:vue",
    "@vue/runtime-dom": "catalog:vue",
    "rimraf": "catalog:",
    "unbuild": "^3.6.1",
    "vitepress": "catalog:",
    "vue": "catalog:"
  }
}
